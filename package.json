{
  "name": "eslint-config-emperor",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "test": "vitest"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "branches": [
      "master",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "devDependencies": {
    "prettier": "2.8.7",
    "table": "^6.8.1",
    "typescript": "5.0.2",
    "vite": "^4.2.1",
    "vitest": "0.29.8"
  },
  "packageManager": "pnpm@8.1.0"
}
